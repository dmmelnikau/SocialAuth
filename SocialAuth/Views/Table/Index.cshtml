@using SocialAuth.Data
@model List<Microsoft.AspNetCore.Identity.IdentityUser>
@{
    ViewBag.Title = "Index";
}

<h2 class="bg-primary m-1 p-1 text-white">Users</h2>

<table class="table table-sm table-bordered">
    <tr>
        <th>Id</th>
        <th>UserName</th>
        <th>Email</th>
    </tr>

    @foreach (var user in Model.OrderBy(x => x.UserName))
    {
<tr>
    <td>@user.Id</td>
    <td>@user.UserName</td>
    <td>@user.Email</td>
</tr>
    }
</table>
